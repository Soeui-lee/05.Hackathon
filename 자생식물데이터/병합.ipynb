    "import json\n",
    "import glob\n",
    "\n",
    "data = []\n",
    "for f in glob.glob(\"CR009_02_*.json\"):\n",
    "    with open(f, encoding=\"utf-8\") as infile:\n",
    "        data.append(json.load(infile))\n",
    "\n",
    "with open(\"CR009_02_*.json\",'w', encoding=\"utf-8\") as outfile:\n",
    "  json.dump(data, outfile, ensure_ascii=False, indent=\"\\t\")"
   ]
  }
